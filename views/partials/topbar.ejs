<script type="text/javascript">
    var href = window.location.href;
    var params = href.split('/');
    var pagename = params.pop();

    switch(pagename) {
        case  'trending':
            $("#trending").addClass('active');
            break;
        case 'popular':
            $("#popular").addClass('active');
            break;
        case 'staffgits':
            $("#staff").addClass('active');
            break;
        case 'recent':
            $("#recent").addClass('active');
            break;
        case 'mostvoted':
            $("#most_vote").addClass('active');
            break;
        default:
            break;            
    } 
</script> 